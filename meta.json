{
  "module_id": "mattmacf:sanding-history-web-app",
  "visibility": "public",
  "url": "https://github.com/mattmacf98/matt-sanding-history-web-app",
  "description": "A web app to monitor sanding",
  "models": [
    {
      "api": "rdk:component:generic",
      "model": "mattmacf:sanding-history-web-app:sanding-history-web-app"
    }
  ],
    "first_run": "",
    "entrypoint": "./bin/sanding-history-web-app",
    "build": {
        "build": "make module",
        "setup": "make setup-linux",
        "arch": [
            "linux/amd64",
            "linux/arm64"
        ],
        "path" : "module.tar.gz"
    },
  "applications": [
    {
      "name": "sanding-monitoring-web-app",
      "type": "single_machine",
      "entrypoint": "dist/index.html"
    }
  ]
}